PREFIX : <http://purl.bdrc.io/ontology/core/>  PREFIX adm: <http://purl.bdrc.io/ontology/admin/>  
PREFIX bdr: <http://purl.bdrc.io/resource/>  PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX skos: <http://www.w3.org/2004/02/skos/core#>  
PREFIX tbr: <http://purl.bdrc.io/ontology/toberemoved/> PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
PREFIX f: <java:io.bdrc.ldsearch.query.functions.> 

#Returns the list of lineage, lineage names
#No arguments

SELECT DISTINCT  ?lineage ?lineage_name 

WHERE   { 
  ?lineage a :Lineage  .
  ?lineage skos:prefLabel ?lineage_name .
}